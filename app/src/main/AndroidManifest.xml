<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.drawcoco">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Inicio"></activity>
        <activity android:name=".CreacionPost" />
        <activity android:name=".AdapterContenedorPost" />
        <activity android:name=".VisualizarPost" />
        <activity android:name=".Filtrar" />
        <activity android:name=".Ajustes" />
        <activity android:name=".formularios.Registro" />
        <activity android:name=".perfiles.PerfilCreador" />
        <activity
            android:name=".Estadisticas"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Galeria"
            android:screenOrientation="portrait" />
        <activity
            android:name=".perfiles.PerfilCliente"
            android:screenOrientation="portrait" />
        <activity
            android:name=".formularios.Login"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>