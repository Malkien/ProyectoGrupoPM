<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<!-- Gracias a esta propiedad cambiamos las propiedades del boton cuando es presionado-->
<item android:state_pressed="true" >
    <shape android:shape="rectangle" >
        <corners
            android:radius="5dp"
            />
        <solid
            android:color="#AFEDF5"
            />
        <stroke
            android:width="2dp"
            android:color="#0D2687"
            />
    </shape>
</item>
<item>
    <shape android:shape="rectangle" >
        <corners
            android:radius="5dp"
            />
        <solid
            android:color="#7CE6F0"
            />
        <stroke
            android:width="1dp"
            android:color="#0D2687"
            />
    </shape>
</item>
</selector>
